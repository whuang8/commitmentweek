<div class="container">
	<div class="row">
		<div class="col-lg-12">
			<div class="page-header">
				<h1>Participants</h1>
				<small>Everyone participating in [commit]ment week</small>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-12">
			<table class="table table-striped table-hover ">
				<thead>
					<tr>
						<th>Name</th>
						<th>Commit Streak</th>
					</tr>
				</thead>
				<tbody>
					<% @users.each do |user| %>
						<tr>
							<td><%= user.name %></td>
							<td><%= user.id %></td>
						</tr>
					<% end %>
				</tbody>
			</table>
		</div>
	</div>
</div>